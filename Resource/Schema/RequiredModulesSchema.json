{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "Name",
      "MinimumVersion"
    ],
    "properties": {
      "Name": {
        "type": "string",
        "minLength": 1
      },
      "MinimumVersion": {
        "type": "string",
        "pattern": "^\\d+\\.\\d+\\.\\d+$"
      }
    },
    "additionalProperties": false
  }
}